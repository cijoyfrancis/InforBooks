<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>

    <!-- Location of input files-->
    <add key="AppDataLocation" value="C:\InforBooks"/>
    
    <!-- File format details - start and end indexes for Name, ISBN and Author.
    Please create this section for each new file type. First part of key (A in this case)
    need to match exactly with the FormatIndicator in the input file-->
    <add key="A:NameStartIndex" value="1"/>
    <add key="A:NameEndIndex" value="20"/>
    <add key="A:ISBNStartIndex" value="21"/>
    <add key="A:ISBNEndIndex" value="41"/>
    <add key="A:AuthorStartIndex" value="42"/>
    <add key="A:AuthorEndIndex" value="62"/>

    <add key="B:NameStartIndex" value="1"/>
    <add key="B:NameEndIndex" value="30"/>
    <add key="B:ISBNStartIndex" value="31"/>
    <add key="B:ISBNEndIndex" value="51"/>
    <add key="B:AuthorStartIndex" value="52"/>
    <add key="B:AuthorEndIndex" value="72"/>

    <!--
    <add key="CJ:NameStartIndex" value="1"/>
    <add key="CJ:NameEndIndex" value="20"/>
    <add key="CJ:ISBNStartIndex" value="21"/>
    <add key="CJ:ISBNEndIndex" value="41"/>
    <add key="CJ:AuthorStartIndex" value="42"/>
    <add key="CJ:AuthorEndIndex" value="62"/>
    -->
    
  </appSettings>
  
</configuration>